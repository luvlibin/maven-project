pipeline {
  agent any
     stages {
		stage ('SCM Checkout'){
		git 'https://github.com/luvlibin/maven-project.git'
		}
         }

		stage ('Code Test'){
		steps {
			withMaven(maven : 'MyMaven'){
			sh 'mvn test'
		    }}
		}

		stage ('install'){
		steps {
			withMaven(maven : 'MyMaven'){
			sh 'mvn install'
		    }}
		}
		}
		
		
		
		
	        
		
